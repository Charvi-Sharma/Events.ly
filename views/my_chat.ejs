          </nav><!-- .navbar -->

        </div>
      </div>

    </div>
  </header><!-- End Header -->



<!-- LEFT SECTION -->

<section id="main-left" class="main-left">
    <!-- header -->
    <div id="header-left" class="header-left">
      
    </div>

    <% if(Object.keys(chat).length === 0) { %>
        <% } else{ chat.forEach(entry => { %>
    <!-- chat list -->
    <div id="chat-list" class="chat-list">
      <!-- user lists -->
      <div id="friends" class="friends">
        <!-- photo -->
        <div class="profile friends-photo">
          <img src="images/ava2.jpg" alt="">
        </div>
        
        <div class="friends-credent">
          <!-- name -->
          <span class="friends-name">
            <% if(entry.msg_sender==my_id) { %>
            <%= entry.msg_reciever%>
                <%} else{ %>
                    <%= entry.msg_sender%>
                    <% } %>
                    <a href="/users/chat_box/<%if(entry.msg_sender==my_id) { %>
                        <%= entry.msg_reciever%>
                            <%} else{ %>
                                <%= entry.msg_sender%>
                                <% } %>">open</a>
                </span>
        </div>
        <!-- notification badge -->
        <span class="badge notif-badge">7</span>
      </div>


    <% }) }%>
  </section>





